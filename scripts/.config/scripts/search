#!/bin/bash
# Used to select and start programs in sway requires fzf, sway, and bash
compgen -c | sort -u | fzf --color=fg:#F7F7F7,bg:#2b2929,hl:#F7F7F7,fg+:#F7F7F7,bg+:235,hl+:#F7F7F7,bg+:#e95420 --color=info:#2b2929,prompt:#2b2929,spinner:135,pointer:#e95420,marker:#2b2929,gutter:#2b2929 --bind='tab:toggle-down,btab:toggle-up' --reverse --border --history=/home/haden/.SHISTORY --inline-info | xargs -r swaymsg -t command exec
